
	CC=nvcc
	CFLAGS=-I.
	DEPS =
	OBJ = main.o loadData.o tsne.o tsneCPU.o utils.o gradient_descendCPU.o timers.o gradient_descend.o Frep.o relocateDataCPU.o

	%.o: %.cu
		$(CC) -c -o $@ $< $(CFLAGS)

	%.o: %.cpp
		$(CC) -c -o $@ $< $(CFLAGS)

	fuck: $(OBJ)
		$(CC) -o $@ $^ $(CFLAGS)

	clean:
		rm -f *.o
